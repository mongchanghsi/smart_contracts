{
  "name": "smart_contracts",
  "version": "1.0.0",
  "description": "This repository serves as my practice to develop smart contracts in solidity. Each of these contracts are independent unless I grouped them in sub-folders. Some of the contracts are either followed closey to existing tutorials or inspired by some of them, in which it is credited below.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npx hardhat node",
    "test": "npx hardhat test",
    "deploy:rinkeby": "npx hardhat run scripts/deploy.ts --network rinkeby",
    "deploy:mainnet": "npx hardhat run scripts/deploy.ts --network mainnet",
    "verify:mainnet": "npx hardhat --network mainnet verify",
    "verify:rinkeby": "npx hardhat --network rinkeby verify",
    "new:wallet": "npx hardhat new:wallet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mongchanghsi/smart_contracts.git"
  },
  "keywords": [],
  "author": "",
