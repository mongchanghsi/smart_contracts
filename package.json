  "bugs": {
    "url": "https://github.com/mongchanghsi/smart_contracts/issues"
  },
  "homepage": "https://github.com/mongchanghsi/smart_contracts#readme",
  "dependencies": {
    "@ethersproject/hardware-wallets": "^5.5.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
